{% extends "base.njk" %}
{% block title %}{{ path.split("/")[1] }}{% endblock %}
{% block body %}
    <a href="/">
        <h1>Back</h1>
    </a>
    {% for i in range(1, 26) -%}
        {% if unlock[path.split("/")[1]] >= i %}
            <a href="{{ path.split("/")[1] }}/{{ i }}">[Day {{ i }}]</a>
        {% endif %}
    {%- endfor %}
{% endblock %}
